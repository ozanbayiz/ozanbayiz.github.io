{
    "name": "ozanbayiz.github.io",
    "homepage": "https://ozanbayiz.github.io",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prebuild": "tsx scripts/collect-projects.mts",
        "dev": "pnpm prebuild && next dev --experimental-https",
        "build": "pnpm prebuild && next build && next-image-export-optimizer",
        "analyze": "ANALYZE=true next build",
        "start": "pnpm dlx serve@latest out -l 3000",
        "preview": "pnpm dlx serve@latest out -l 3000",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "typecheck": "tsc --noEmit",
        "predeploy": "pnpm run build",
        "deploy": "gh-pages -d out",
        "depcheck": "depcheck",
        "ts-prune": "ts-prune -p tsconfig.json"
    },
    "dependencies": {
        "@mdx-js/react": "^3.1.0",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-dialog": "^1.1.14",
